 <%= simple_form_for [@restaurant, @review] do |f| %>
        <div class="mb-3">
          <%= f.input :rating, class: 'form-control' %>
        </div>
        <div class="mb-3">
          <%= f.input :content, class: 'form-control' %>
        </div>
        <div class="text-center">
          <%= f.button :submit, class: 'btn btn-primary' %>
        </div>
      <% end %>
